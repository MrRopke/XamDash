<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamDash.Views.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="TestLabel" Text="Welcome to Xamarin Forms!" />


            <ListView x:Name="CoinView">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding CoinName}" />
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>

        

    </ContentPage.Content>
</ContentPage>